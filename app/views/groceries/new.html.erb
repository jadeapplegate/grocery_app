<h1>Add an item to your GroceryList:</h1></br>
<I><%= form_for @grocery do |f| %>

  <%= label_tag(:item, "Item:") %></I>
  <%= f.text_field :item , :class => "form-control" %></br>

  <I><%= label_tag(:details, "Details:") %> </I>
  <%= f.text_area :details, :class => "form-control" %></br>
  <%= f.submit "Create", :class => "btn btn-primary btn-default" %>
<% end %>

